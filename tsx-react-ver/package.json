{
  "name": "smonthl-react",
  "version": "2.0.7",
  "description": "SmonthlAPI - Beautiful frosted glass UI with liquid glass text, extended creative syntax, external imports (React/TypeScript version)",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "start": "NODE_ENV=development electron electron-main.js",
    "start:prod": "electron electron-main.js",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build --mode lib",
    "build:electron": "npm run build && electron-builder",
    "package": "npm run build && electron-builder",
    "package:all": "npm run build && electron-builder --linux --mac --win",
    "preview": "vite preview",
    "test": "jest",
    "prepublishOnly": "npm run build:lib",
    "publish:npm": "npm publish --access public"
  },
  "keywords": [
    "smonthl",
    "smonthl-api",
    "liquid-glass",
    "frosted-glass",
    "glassmorphism",
    "jelly-physics",
    "elastic-ui",
    "magnetic-ui",
    "cursor-effects",
    "react",
    "typescript",
    "react-component",
    "ui-component",
    "glass-effect",
    "backdrop-filter",
    "magnifying-lens",
    "tsx",
    "hooks",
    "configuration-api"
  ],
  "author": "Arthurc1Moude <arch_cheng@163.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moude-AI/smonthl_api.git",
    "directory": "tsx-react-ver"
  },
  "bugs": {
    "url": "https://github.com/Moude-AI/smonthl_api/issues"
  },
  "homepage": "https://github.com/Moude-AI/smonthl_api#readme",
  "files": [
    "dist",
    "src",
    "glass-config.json",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "build": {
    "appId": "com.liquidglass.react",
    "productName": "Liquid Glass React",
    "directories": {
      "output": "dist-electron",
      "buildResources": "."
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "glass-config.json",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron-main.js"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "zip",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "portable",
          "arch": ["x64", "ia32"]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64", "arm64", "armv7l"]
        },
        {
          "target": "deb",
          "arch": ["x64", "arm64", "armv7l"]
        }
      ],
      "category": "Utility"
    }
  }
}
