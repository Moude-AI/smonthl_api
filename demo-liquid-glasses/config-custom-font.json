{
  "componentType": "custom-font",
  "glass": {
    "transparency": 6,
    "blur": 60,
    "magnifyingBlur": 30,
    "magnifyingBrightness": 115,
    "lensSize": 40,
    "borderRadius": 24,
    "width": 400,
    "height": 180
  },
  "content": {
    "type": "text",
    "title": "Playfair Display",
    "subtitle": "Elegant serif font with custom typography"
  },
  "typography": {
    "fontFamily": "'Playfair Display', serif",
    "titleSize": "42px",
    "subtitleSize": "16px",
    "titleWeight": "700",
    "subtitleWeight": "400",
    "letterSpacing": "-1px",
    "lineHeight": "1.2"
  },
  "externalLibraries": [
    "https://fonts.googleapis.com/css2?family=Playfair+Display:wght@400,700&display=swap"
  ],
  "jelly": {
    "enabled": true,
    "elasticity": 0.6,
    "friction": 0.85,
    "bounceIntensity": 15,
    "wobbleSpeed": 0.3,
    "magneticRange": 150,
    "magneticStrength": 0.3
  },
  "lighting": {
    "cursorFollowEnabled": true,
    "followDistance": 200,
    "lightIntensity": 0.8,
    "lightSize": 120,
    "lightColor": "255, 255, 255"
  },
  "draggable": true,
  "magnifyingLens": true,
  "backgrounds": [
    "https://images.unsplash.com/photo-1441974231531-c6227db76b6e?w=1920&q=80"
  ],
  "backgroundChangeInterval": 10000
}
